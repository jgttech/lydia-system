#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/odysseus/Workspaces/lydia/src/apps/trainer/react-web/.dist/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules/jsesc/bin/jsesc/node_modules:/home/odysseus/Workspaces/lydia/src/apps/trainer/react-web/.dist/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules/jsesc/bin/node_modules:/home/odysseus/Workspaces/lydia/src/apps/trainer/react-web/.dist/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules/jsesc/node_modules:/home/odysseus/Workspaces/lydia/src/apps/trainer/react-web/.dist/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules:/home/odysseus/Workspaces/lydia/src/apps/trainer/react-web/.dist/node_modules/.pnpm/registry.npmjs.org/jsesc/node_modules:/home/odysseus/Workspaces/lydia/src/apps/trainer/react-web/.dist/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/odysseus/Workspaces/lydia/src/apps/trainer/react-web/.dist/node_modules/.pnpm/node_modules:/home/odysseus/Workspaces/lydia/src/apps/trainer/react-web/.dist/node_modules:/home/odysseus/Workspaces/lydia/src/apps/trainer/react-web/node_modules:/home/odysseus/Workspaces/lydia/src/apps/trainer/node_modules:/home/odysseus/Workspaces/lydia/src/apps/node_modules:/home/odysseus/Workspaces/lydia/src/node_modules:/home/odysseus/Workspaces/lydia/node_modules:/home/odysseus/Workspaces/node_modules:/home/odysseus/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../../jsesc/2.5.2/node_modules/jsesc/bin/jsesc" "$@"
else 
  exec node  "$basedir/../../../../../../../../jsesc/2.5.2/node_modules/jsesc/bin/jsesc" "$@"
fi
